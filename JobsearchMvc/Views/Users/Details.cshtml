@model Dal.Models.User

<h2>User Details</h2>

<div>
    <h4>User</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">UserId</dt>
        <dd class="col-sm-10">@Model.UserId</dd>

        <dt class="col-sm-2">UserTypeId</dt>
        <dd class="col-sm-10">@Model.UserTypeId</dd>

        <dt class="col-sm-2">UserName</dt>
        <dd class="col-sm-10">@Model.UserName</dd>

        <dt class="col-sm-2">Password</dt>
        <dd class="col-sm-10">@Model.Password</dd>

        <dt class="col-sm-2">EmailAddress</dt>
        <dd class="col-sm-10">@Model.EmailAddress</dd>

        <dt class="col-sm-2">ContactNo</dt>
        <dd class="col-sm-10">@Model.ContactNo</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.UserId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>