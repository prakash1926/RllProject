@model Dal.Models.UserType

<h2>Delete UserType</h2>

<h3>Are you sure you want to delete this UserType?</h3>
<div>
    <h4>UserType</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">UserTypeId</dt>
        <dd class="col-sm-10">@Model.UserTypeId</dd>

        <dt class="col-sm-2">UserTypeText</dt>
        <dd class="col-sm-10">@Model.UserTypeText</dd>
    </dl>
</div>
<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="UserTypeId" />
    <div class="form-group">
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </div>
</form>